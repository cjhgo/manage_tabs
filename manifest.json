{
  "browser_action": {
    "browser_style": true,
    "default_title": "List all tabs",
    "default_popup": "/popup/tabs.html",
    "default_icon": {
      "19": "icons/default19.png",
      "38": "icons/default38.png"
    }
  },
  "description": "List all tabs.",
  "icons": {
    "48": "icons/cookie.png",
    "96": "icons/cookie@2x.png"
  },
  "homepage_url": "https://github.com/cjhgo/manage_tabs",
  "manifest_version": 2,
  "name": "List tabs",
  "version": "2.0",
  "permissions": ["cookies","<all_urls>","tabs"]
}
